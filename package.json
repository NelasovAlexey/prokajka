{
  "name": "foundation-xy",
  "version": "1.1.0",
  "private": true,
  "browserslist": [
    "last 2 versions",
    "ie >= 9",
    "android >= 4.4",
    "ios >= 7"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "semi": true
  },
  "engines": {
    "node": ">= 6.9.4"
  },
  "scripts": {
    "build": "webpack --progress --config build/webpack.prod.js",
    "build:production": "yarn run build --env production",
    "build:production:analyze": "yarn run build:production --analyze",
    "start": "node build/watch.js",
    "lint": "yarn run -s lint:scripts && yarn run -s lint:styles && yarn run -s lint:php",
    "lint:php": "vendor/bin/phpcs",
    "lint:scripts": "eslint assets/scripts",
    "lint:styles": "stylelint \"assets/styles/**/*.scss\"",
    "fix:php": "vendor/bin/php-cs-fixer --verbose fix --config .php_cs_fixer",
    "fix:scripts": "prettier --write \"assets/scripts/**/*.js\"",
    "fix:styles": "prettier --write \"assets/**/*.scss\""
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "babel-minify-webpack-plugin": "^0.3.1",
    "browser-sync": "^2.26.14",
    "browser-sync-webpack-plugin": "^2.3.0",
    "browserslist": "^4.16.1",
    "cache-loader": "^4.1.0",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-globs-webpack-plugin": "^0.3.0",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "debug": "^4.3.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-webpack-plugin": "^2.4.3",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "import-glob": "^1.5.0",
    "mini-css-extract-plugin": "^1.3.5",
    "node-sass": "^4.14.1",
    "object-fit-images": "^3.2.4",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.2.0",
    "postcss-safe-parser": "^5.0.2",
    "prettier": "^2.0.4",
    "ramda": "^0.27.1",
    "resolve-url-loader": "^2.3.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "select2": "^4.1.0-rc.0",
    "source-map-resolve": "^0.6.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.3.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.1.2",
    "stylelint-webpack-plugin": "^2.1.1",
    "url-loader": "^4.1.1",
    "vanilla-lazyload": "^17.3.0",
    "webpack": "^4.46.0",
    "webpack-assets-manifest": "^4.0.1",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-filter-warnings-plugin": "^1.2.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.7.3",
    "whatwg-fetch": "^3.5.0",
    "yargs": "^16.2.0"
  },
  "dependencies": {
    "@fancyapps/fancybox": "^3.5.7",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "foundation-sites": "^6.6.3",
    "jarallax": "^1.12.5",
    "jquery": "^3.5.1",
    "jquery-match-height": "^0.7.2",
    "scroll-out": "^2.2.12",
    "slick-carousel": "^1.8.1",
    "what-input": "^5.2.10"
  }
}
